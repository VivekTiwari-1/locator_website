function getMembershipPriceMonthly(){try{let priceCountry;if(typeof AHE.membershipPrices[AHE.ipCountry]=='object'){priceCountry=AHE.ipCountry;}else if(AHE.isEU){priceCountry='eu';}else{priceCountry='us';}
let pricing=AHE.membershipPrices[priceCountry];pricing.price=pricing.price*1;let curspace='';if(pricing.cursymb.replace(/[^a-zA-Z]/,'')==pricing.cursymb){curspace=' ';}
if(pricing.price.toFixed(0)!=pricing.price){pricing.price=pricing.price.toFixed(2);}
return pricing.cursymb+curspace+pricing.price;}catch(e){console.error('Could not set localized membership pricing.');}}
function dragSort(selector,callback){let items=document.querySelectorAll(selector),current=null;for(let i of items){i.draggable=true;i.ondragstart=(ev)=>{current=i;for(let it of items){if(it!=current){it.classList.add("hint");}}};i.ondragenter=(ev)=>{if(i!=current){i.classList.add("active");}};i.ondragleave=()=>{i.classList.remove("active");};i.ondragend=()=>{for(let it of items){it.classList.remove("hint");it.classList.remove("active");}};i.ondragover=(evt)=>{evt.preventDefault();};i.ondrop=(evt)=>{evt.preventDefault();if(i!=current){let currentpos=0,droppedpos=0;for(let it=0;it<items.length;it++){if(current==items[it]){currentpos=it;}
if(i==items[it]){droppedpos=it;}}
if(currentpos<droppedpos){i.parentNode.insertBefore(current,i.nextSibling);}else{i.parentNode.insertBefore(current,i);}
if(typeof callback=='function'){callback(null);}}};}}
function human_verify(force_check=false){if(!getCookie('cf_turnstile_attempt')){if(force_check||!is_human_verified()){async_load_js('https://challenges.cloudflare.com/turnstile/v0/api.js',()=>{setCookie('cf_turnstile_attempt','1');let cf_ts_div=document.createElement('div');cf_ts_div.id='cf_turnstile';document.getElementById('footer').appendChild(cf_ts_div);turnstile.render('#'+cf_ts_div.id,{sitekey:AHE.cfTurnstileKey,callback:function(token){console.log('Cloudflare Turnstile verification successful.');set_human_verified();},});});}}else{console.warn('Cloudflare Turnstile verification already attempted in this session.');}}
